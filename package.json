{
  "name": "@owl-93/themed-browser-bar",
  "version": "1.0.3",
  "description": "A lightweight backage providing a hook to manage theme meta tag in your document head. It is meant to allow you to dynamically update the theme color of your website so that you can control the color of the browser url bar on certain mobile browsers.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "test": "npm test",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/owl-93/themed-browser-bar.git"
  },
  "keywords": [
    "theme",
    "browser",
    "bar",
    "color"
  ],
  "author": "Stephen Schmidt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/owl-93/themed-browser-bar/issues"
  },
  "homepage": "https://github.com/owl-93/themed-browser-bar#readme",
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "typescript": "^4.4.4"
  }
}
